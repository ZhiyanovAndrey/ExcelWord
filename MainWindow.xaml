<Window x:Class="ExcelWord.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelWord"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="800">
    <StackPanel>
        <Menu Background="gray">
            <MenuItem Header="Открыть" Click="MenuItem_Click"/>
            
        </Menu>
        <Label Content="Введите ссылку на файл Excel"/>
        <TextBox x:Name="textbox" Background="Yellow"/>
        <Button Content="Отчет в Word" Background="Gray" Click="Button_Click"/>
        <DataGrid x:Name="datagrid1" Height="900"/>
    </StackPanel>
</Window>
